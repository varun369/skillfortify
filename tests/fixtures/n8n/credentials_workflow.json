{
  "name": "Credentials Workflow",
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Trigger",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1,
      "position": [250, 300],
      "parameters": {
        "rule": {
          "interval": [{"field": "hours", "hoursInterval": 1}]
        }
      }
    },
    {
      "id": "2",
      "name": "Slack Notify",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 2,
      "position": [450, 300],
      "parameters": {
        "channel": "#alerts",
        "text": "Hourly status report"
      },
      "credentials": {
        "slackApi": {
          "id": "20",
          "name": "Slack Bot Token"
        }
      }
    },
    {
      "id": "3",
      "name": "Gmail Send",
      "type": "n8n-nodes-base.gmail",
      "typeVersion": 2,
      "position": [650, 200],
      "parameters": {
        "sendTo": "admin@company.com",
        "subject": "Status Report",
        "message": "All systems operational"
      },
      "credentials": {
        "gmailOAuth2": {
          "id": "21",
          "name": "Gmail OAuth"
        }
      }
    },
    {
      "id": "4",
      "name": "AWS S3 Upload",
      "type": "n8n-nodes-base.awsS3",
      "typeVersion": 1,
      "position": [650, 400],
      "parameters": {
        "operation": "upload",
        "bucketName": "company-backups",
        "fileName": "report.json"
      },
      "credentials": {
        "aws": {
          "id": "22",
          "name": "AWS Production"
        }
      }
    },
    {
      "id": "5",
      "name": "MySQL Query",
      "type": "n8n-nodes-base.mySql",
      "typeVersion": 2,
      "position": [450, 500],
      "parameters": {
        "operation": "select",
        "table": "metrics",
        "options": {}
      },
      "credentials": {
        "mySql": {
          "id": "23",
          "name": "MySQL Prod"
        }
      }
    }
  ],
  "connections": {
    "Schedule Trigger": {
      "main": [
        [
          {"node": "Slack Notify", "type": "main", "index": 0},
          {"node": "MySQL Query", "type": "main", "index": 0}
        ]
      ]
    },
    "Slack Notify": {
      "main": [
        [
          {"node": "Gmail Send", "type": "main", "index": 0},
          {"node": "AWS S3 Upload", "type": "main", "index": 0}
        ]
      ]
    }
  }
}
